{
    "cSpell.words": ["xabcdef"],
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.formatOnType": true,
    "files.watcherExclude": {
        "**/target": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/bin": true,
        "**/build": true
    },
    "java.autobuild.enabled": true,
    "java.cleanup.actions": [
        "addDeprecated",
        "addFinalModifier",
        "addOverride",
        "instanceofPatternMatch",
        "invertEquals",
        "lambdaExpression",
        "lambdaExpressionFromAnonymousClass",
        "stringConcatToTextBlock",
        "switchExpression",
        "tryWithResource"
    ],
    "java.codeGeneration.hashCodeEquals.useInstanceof": true,
    "java.codeGeneration.hashCodeEquals.useJava7Objects": true,
    "java.codeGeneration.toString.codeStyle": "STRING_BUILDER_CHAINED",
    "java.compile.nullAnalysis.mode": "automatic",
    "java.completion.enabled": true,
    "java.completion.matchCase": "off",
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.dependency.packagePresentation": "hierarchical",
    "java.format.enabled": true,
    "java.format.onType.enabled": true,
    "java.format.settings.url": ".vscode/formattingStyle.xml",
    "java.format.settings.profile": "MyStyle",
    "java.inlayHints.parameterNames.enabled": "none",
    "java.saveActions.organizeImports": true,
    "java.server.launchMode": "Standard",
    "java.compile.nullAnalysis.nonnull": ["jakarta.annotation.Nonnull"],
    "java.compile.nullAnalysis.nullable": ["jakarta.annotation.Nullable"]
}
